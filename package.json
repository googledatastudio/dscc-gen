{
  "name": "@google/dscc-gen",
  "version": "2.0.4",
  "repository": "googledatastudio/dscc-gen",
  "description": "Create component & connector projects with sane defaults.",
  "main": "build/index.js",
  "bin": "build/index.js",
  "engines": {
    "node": ">=7.10.0",
    "npm": ">=5.2.0"
  },
  "author": "Matt Hamrick <matthewjhamrick@gmail.com>",
  "license": "Apache-2.0",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watchAll --coverage",
    "lint": "tslint -c tslint.json -p tsconfig.json \"src/**/*.ts\"",
    "prettier": "prettier --write '{src,templates,test}/**/*.{ts,js}'",
    "prettier:check": "prettier --check '{src,templates,test}/**/*.{ts,js}'",
    "test:ci": "yarn prettier:check && yarn lint && yarn build:check && yarn test && ./test/integration_tests.sh"
  },
  "dependencies": {
    "@types/mz": "^0.0.32",
    "argparse": "^1.0.10",
    "bluebird": "^3.5.1",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "cli-spinner": "^0.2.10",
    "dotf": "^1.2.0",
    "execa": "^1.0.0",
    "inquirer": "^6.2.1",
    "insight": "^0.10.1",
    "mz": "^2.7.0",
    "recursive-readdir": "^2.2.2",
    "terminal-link": "^1.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@google/clasp": "^2.1.0",
    "@types/argparse": "^1.0.36",
    "@types/bluebird": "^3.5.26",
    "@types/cli-spinner": "^0.2.0",
    "@types/execa": "^0.9.0",
    "@types/inquirer": "^0.0.44",
    "@types/insight": "^0.8.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.4",
    "@types/recursive-readdir": "^2.2.0",
    "@types/terminal-link": "^1.1.0",
    "@types/uuid": "^3.4.4",
    "jest": "^24.5.0",
    "prettier": "^1.11.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  }
}
